{
    "name": "mabrouk/permission",
    "description": "a laravel package to handle dealing with permissions in custom groups",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ahmed Mabrouk",
            "email": "a.mabrouk25@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "laravel/framework": ">=9",
        "mabrouk/translatable": ">=1",
        "mabrouk/filterable": ">=1"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0@dev"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mabrouk\\Permission\\PermissionServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Mabrouk\\Permission\\": "src/",
            "Database\\Factories\\": "src/database/factories/",
            "Database\\Seeders\\": "src/database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
